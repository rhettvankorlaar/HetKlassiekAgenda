    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.4",
    "dayjs": "^1.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
